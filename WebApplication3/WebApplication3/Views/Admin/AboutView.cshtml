@model IEnumerable<WebApplication3.Admin_Home_Result>

<p>
   <h2><b>Currently Allocated Task(s)</b></h2>
</p>
<br>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ManagerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProgressPercent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Deadline)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ManagerId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProgressPercent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deadline)
        </td>
        
        
    </tr>
}

</table>
